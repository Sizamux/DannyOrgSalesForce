public with sharing class Calculadora {
    public Integer soma(Integer n1, Integer n2){
        return n1+n2;
    }
    public Integer subtracao(Integer n1, Integer n2){
        return n1-n2;
    }
    public Integer divisao(Integer n1, Integer n2){
        return n1/n2;
    }
    public Integer multiplicacao(Integer n1, Integer n2){
        return n1*n2;
    }
}